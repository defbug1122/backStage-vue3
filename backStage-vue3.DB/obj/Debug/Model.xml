		<Element Type="SqlProcedure" Name="[dbo].[pro_bs_editUser]">
			<Property Name="BodyScript">
				<Value><![CDATA[
BEGIN
    SET NOCOUNT ON;

    -- 檢查是否正在修改自己的帳號
    IF @UserName = @CurrentUserName
    BEGIN
        SELECT 'Cannot edit your own account' AS Message;
        RETURN;
    END

    -- 更新用戶密碼和權限
    UPDATE t_account
    SET f_password = @NewPassword, f_permission = @NewPermission
    WHERE f_username = @UserName;

    IF @@ROWCOUNT = 0
    BEGIN
        SELECT 'User not found' AS Message;
    END
    ELSE
    BEGIN
        SELECT 'User updated successfully' AS Message;
    END
END]]></Value>
			</Property>
